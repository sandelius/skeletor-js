{
  "name": "skeletor-js",
  "version": "1.0.0",
  "description": "Skeleton package",
  "author": "Tobias Sandelius <tobias@sandeli.us>",
  "license": "MIT",
  "main": "lib/index.js",
  "repository": "github:sandelius/skeletor-js",
  "scripts": {
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:ci": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "standard": "standard --env jest"
  },
  "keywords": [
    "Skeleton"
  ],
  "engines": {
    "node": ">=10.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "jest": "^22.4.3",
    "standard": "^11.0.1"
  },
  "jest": {
    "testMatch": [
      "**/*-test.js"
    ],
    "setupFiles": [
      "./test/test-helper.js"
    ]
  }
}
